{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "plugins": [
        "security"
    ],
    "extends": [
        "airbnb-base",
        "plugin:security/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "camelcase": [
            { "ignoreDestructuring": false } 
        ],
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}